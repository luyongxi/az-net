train_net: "models/Pascal/VGG16_skip/frcnn/finetune/train.prototxt"
base_lr: 0.001
clip_gradients: 20
lr_policy: "step"
gamma: 0.2
stepsize: 30000
display: 20
average_loss: 100
# iter_size: 1
momentum: 0.9
weight_decay: 0.0005
# We disable standard caffe solver snapshotting and implement our own snapshot
# function
snapshot: 0
# We still use the snapshot prefix, though
snapshot_prefix: "vgg16_fast_rcnn_skip_finetune"
#debug_info: true
